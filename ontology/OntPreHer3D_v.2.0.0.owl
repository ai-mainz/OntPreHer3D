        <rdfs:label xml:lang="el">Υλικό Μόρφωμα</rdfs:label>
        <rdfs:label xml:lang="ru">Физичeский Признак</rdfs:label>
        <rdfs:label xml:lang="zh">物理特征</rdfs:label>
