            </rdf:Description>
        </rdfs:subPropertyOf>
        <rdfs:label xml:lang="fr">est inclus dans</rdfs:label>
        <rdfs:label xml:lang="en">is incorporated in</rdfs:label>
        <rdfs:label xml:lang="ru">инкорпорирован в</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/P166_was_a_presence_of -->

