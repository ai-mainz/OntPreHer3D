    <!-- http://www.cidoc-crm.org/extensions/crminf/J30_has_domain -->

    <owl:ObjectProperty rdf:about="http://www.cidoc-crm.org/extensions/crminf/J30_has_domain">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/extensions/crminf/J28_contains_entity_reference"/>
